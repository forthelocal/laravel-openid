{
    "name": "forthelocal/laravel-openid",
    "description": "laravel plugin to handle openid.",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^7.1.8",
        "illuminate/database": "^5.5",
        "lcobucci/jwt": "^3.2",
        "guzzlehttp/guzzle": "6.3.*",
        "phpseclib/phpseclib": "2.0.*"
    },
    "autoload": {
        "psr-4": {
            "ForTheLocal\\Laravel\\OpenID\\": "src/"
        }
    },
    "minimum-stability": "stable",
    "require-dev": {
        "phpunit/phpunit": "^6.3",
        "orchestra/testbench": "^3.5",
        "orchestra/database": "^3.5",
        "mockery/mockery": "^1.0"
    },
    "autoload-dev": {
        "psr-4": {
            "ForTheLocal\\Test\\": "tests/"
        }
    }
}
